{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "iam:PassRole"
            ],
            "Resource": "*",
            "Effect": "Allow"
        },
        {
            "Action": [
                "cloudwatch:*",
                "s3:*",
                "ecs:*",
                "ecr:*",
                "codebuild:*",
                "codedeploy:*"
            ],
            "Resource": "*",
            "Effect": "Allow"
        }
    ]
}